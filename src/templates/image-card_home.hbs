{{#each this}}

  <li class='card__item list' data-id='{{id}}'>
    <article class='card'>
      <a href='' class='card__link link'>
        <div class='card-img-container'>
          <img src='{{webformatURL}}' alt='' data-id='{{id}}' class='card__image' /></div>

        <div class='card__description'>
          <h2 class='card__name'>{{type}}</h2>
          <p class='card__info'>{{user}} | {{user}}</p>
        </div>
      </a>
    </article>
  </li>
{{/each}}